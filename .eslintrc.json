{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "func-names":"off",
        "no-param-reassign":"off",
        "no-console": "off",
        "import/no-extraneous-dependencies": "off",
        "no-underscore-dangle": "off",
        "no-unused-vars":"off",
        "linebreak-style": "off",
        "import/order":"off",
        "import/no-import-module-exports":"off",
        "no-restricted-globals":"off"
    }
}
